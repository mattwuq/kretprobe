echo preempt=0
for l in 96 128 192; do
    for j in 1 2 4 6 8 12 16 24 32 48 64 80 96; do
        for i in zz fl ra sa saca pc saea cq; do
            for k in 1 2 3 4 5; do
                echo insmod krp_$i.ko interval=10 threads=$j cycleus=0 max=$l preempt=0
                dmesg -c > /dev/null; insmod krp_$i.ko interval=10 threads=$j cycleus=0 max=$l preempt=0 2>&1; dmesg -c
             done
        done
    done
done

echo preempt=1 cycleus=0
for l in 96 128 192; do
    for j in 1 2 4 6 8 12 16 24 32 48 64 80 96; do
        for i in zz fl ra sa saca pc saea; do
            for k in 1 2 3 4 5; do
                echo insmod krp_$i.ko interval=10 threads=$j cycleus=0 max=$l preempt=1
                dmesg -c > /dev/null; insmod krp_$i.ko interval=10 threads=$j cycleus=0 max=$l preempt=1 2>&1; dmesg -c
             done
        done
    done
done

echo preempt=1 cycleus=1
for l in 96 128 192; do
    for j in 1 2 4 6 8 12 16 24 32 48 64 80 96; do
        for i in zz fl ra sa saca pc saea; do
            for k in 1 2 3 4 5; do
                echo insmod krp_$i.ko interval=10 threads=$j cycleus=1 max=$l preempt=1
                dmesg -c > /dev/null; insmod krp_$i.ko interval=10 threads=$j cycleus=1 max=$l preempt=1 2>&1; dmesg -c
             done
        done
    done
done
